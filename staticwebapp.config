{
  "routes": [
    {
      "route": "/login",
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout",
      "statusCode": 302
    },
    {
      "route": "/api/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/*",
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    }
  }
}
